{
  "name": "jpuskar-horizon",
  "version": "8.1.0.1",
  "author": "Puppet Labs and OpenStack Contributors",
  "summary": "Puppet module for OpenStack Horizon",
  "license": "Apache-2.0",
  "source": "git://github.com/openstack/puppet-horizon.git",
  "project_page": "https://github.com/jpuskar/puppet-horizon/commit/c3e2e961b747acb52c5a65163d0787f17f959cf0",
